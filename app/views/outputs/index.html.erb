<h1>Outputs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outputs.each do |output| %>
      <tr>
        <td><%= link_to 'Show', book_output_path(output.book_id, output.id) %></td>
        <td><%= link_to 'Edit', edit_book_output_path(output.book_id, output.id) %></td>
        <td><%= link_to 'Destroy', book_output_path(output.book_id, output.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Output', new_output_path %>
