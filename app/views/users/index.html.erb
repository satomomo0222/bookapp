<% provide(:title, "マイページ ") %>
<%= current_user.email %>
<h1>マイページ</h1>
<p>ここに作ります</p>

<% @users.each do |user| %>
  <figure class="image is-128x128">
    <%= link_to user_path(user) do %>
      <%= attachment_image_tag user, :profile_image, fallback: "no-image" %>
    <% end %>
  </figure>
  <%= user.email %>
<% end %>