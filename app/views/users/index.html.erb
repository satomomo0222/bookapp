<% provide(:title, "ユーザー一覧") %>
<%= current_user.email %>
<h1>ユーザー一覧</h1>


<% @users.each do |user| %>
  <figure class="image is-128x128">
    <%= link_to user_path(user) do %>
      <%= attachment_image_tag user, :profile_image, fallback: "no-image" %>
    <% end %>
  </figure>
  <%= @user.id %>
  <%= @user.email %>
  <%= @user.username %>
  <%= @user.profile %>
  <%= @user.twitter %>
  <%= @user.instagram %>
  <%= @user.facebook %>
  <%= @user.website %>
<% end %>